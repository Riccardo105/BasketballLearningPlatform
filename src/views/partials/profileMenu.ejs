<link href="/css/main.css" rel="stylesheet">



<div id="profileMenu" class=" flex flex-col flex-wrap flex-grow w-full h-30px text-right hidden xl:w-1/3 xl:px-20 xl:ml-auto ">
    <% if (!isLoggedIn) { %>
        <button type="button" class=" loginBtn bg-slate-600 text-white hover:bg-white hover:text-slate-950 text-right border-t-2 border-black ">Log-in </button>
      <% } else { %>
        <button type="button" class="signoutBtn bg-slate-600 text-white hover:bg-white hover:text-slate-950 text-right border-t-2 border-black ">Sign Out </button>
      <% } %>
    <button type="button"  class=" profileBtn bg-slate-600 text-white hover:bg-white hover:text-slate-950 text-right rounded-b-md ">Profile </button>
   
</div>