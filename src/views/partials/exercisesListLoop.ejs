

 <!-- displaying rendered exercises-->
 <% for(var exercise of exercises) { %>
    <a href="/exercise/<%= exercise._id %>"
        class=" exercise flex flex-col bg-white drop-shadow-lg  rounded-md m-2 cursor-pointer border-b-2 border-black hover:bg-cyan-200"
            data-category="<%= exercise.category %>"
            data-skillLevel="<%= exercise.skillLevel %>">
        <h1 class="text-black text-lg text-left p-1 cursor-pointer w-full"> <%= exercise.title %></h1>
        <div class="flex flex-row justify-start">
            <p class="text-sm text-start text-black p-2 border-r-2 w-fit"> <%= exercise.category %></p>
            <p class="text-sm text-start text-black p-2 border-r-2 w-fit"> <%= exercise.skillLevel %></p>
        </div>
    </a>
<% } %>